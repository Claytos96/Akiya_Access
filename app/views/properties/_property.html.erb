<div class="card-search-result">
  <%= cl_image_tag(property.photo.key, width: 400, height: 300, crop: :fill) %>
  <span class="prefecture">
    <p><%= property.city %>,
    <%= property.prefecture %></span></p>
  <p class="property-info">
    <%= "#{property.bedrooms} bedrooms | #{number_to_currency(property.price)}" %></p>
  <p><%= link_to "More Info", property_path(property), class: "button-outline" %></p>
</div>
